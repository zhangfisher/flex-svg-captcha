{
  "name": "svg-captcha-embed",
  "version": "1.4.0",
  "description": "generate svg captcha in node.js or express.js with embeded font",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "require": "dist/index.js",
  "types": "dist/index.d.ts",
  "access": "public",
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "xo",
    "build": "rollup -c",
    "changeset": "changeset",
    "release": "changeset version && changeset publish"
  },
  "packageManager": "pnpm@9.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangfisher/svg-captcha-embed"
  },
  "keywords": [
    "captcha",
    "svg",
    "node captcha",
    "captcha generator",
    "captcha alternative"
  ],
  "engines": {
    "node": ">=4.x"
  },
  "license": "MIT",
  "homepage": "https://github.com/zhangfisher/svg-captcha-embed#readme",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.0",
    "opentype.js": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@changesets/cli": "^2.27.11",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^21.1.0",
    "jest-environment-node-debug": "^2.0.0",
    "rollup": "^4.29.1",
    "rollup-plugin-clear": "^2.0.7",
    "unplugin-embed": "^0.0.3",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "jest"
    ],
    "rules": {
      "linebreak-style": [
        0
      ]
    }
  },
  "files": [
    "dist",
    "*.md",
    "LICENSE.md"
  ]
}
